/* pages/photoStore/photoStore.wxss */

.imageWrapper {
  background-color: rgb(246, 246, 246);
}

.foreImg {
  width: 100vw;
  height: 200rpx;
  animation: scale 6s linear infinite;
}

.image-swiper {
  width: 100vw;
  height: 600rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-300rpx);
}

.slide-image {
  width: 100vw;
  height: 600rpx;
  box-shadow: 0 0 10px rgb(7, 17, 27, 0.6);
}

.flower {
  position: absolute;
  z-index: 10;
}

