@keyframes scale {
  0% {
    transform: scale(1);
  }

  25% {
    transform: scale(1.05);
  }

  50% {
    transform: scale(1.1);
  }

  75% {
    transform: scale(1.05);
  }

  100% {
    transform: scale(1);
  }
}

@keyframes snow1 {
  0% {
    top: -10px;
    rotate: 0;
    left: 0;
  }

  100% {
    left: 20px;
    top: 100vh;
    rotate: 360deg;
  }
}

@keyframes snow2 {
  0% {
    top: -10px;
    rotate: 0;
    left: 0;
  }

  100% {
    left: 50px;
    top: 100vh;
    rotate: 360deg;
  }
}

@keyframes snow3 {
  0% {
    top: -10px;
    rotate: 0;
  }

  100% {
    top: 100vh;
    rotate: 360deg;
  }
}

@keyframes qiqiu {
  0%{
    top: 100vh;
  }
  100% {
    top: -30px;
    scale: 1.1;
  }
}


